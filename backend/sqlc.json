{
  "version": "2",
  "sql": [{
    "schema": "../db/migrations",
    "queries": "data/queries",
    "engine": "postgresql",
    "gen": {
      "go": {
        "sql_package": "pgx/v5",
        "out": "internal/data/repo",
        "emit_json_tags": true,
        "json_tags_case_style": "pascal",
        "emit_interface": true,
        "emit_pointers_for_null_types": true,
        "emit_enum_valid_method": true
      }
    }
  }]
}
